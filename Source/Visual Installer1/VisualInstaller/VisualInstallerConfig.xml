        <Feature Required="false" Key="HttpTracing" DisplayName="Tracing" RoleSwitch="IIS-HttpTracing" />
        <Feature Required="false" Key="CustomLogging" DisplayName="Custom Logging" RoleSwitch="IIS-CustomLogging" />
